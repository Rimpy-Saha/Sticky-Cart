{% if is_front == false %} {{ custom_breadcrumb }} {% endif %}
{% endif %}
{% if node.id == '2320' or node.id == '2332' or node.id == '2070' or node.id == '2506' or node.id == '2016' or node.id == '1752' or node.id == '2514' %}
  <div class="container contextual-links-region">
{% elseif content_type == 'event' or content_type == 'page' or content_type == 'product_category_list' or content_type ==
