        {{ page.content }}
        {% endif %}
        {# magbead promo popup #}
        {% if magbead_dnase_promo_info %}
        <div id="magbead-promo-overlay" class="fixed">
          <div class="magbead-promo-wrapper">
            <h2 class="center">Bundle and Save!</h2>
