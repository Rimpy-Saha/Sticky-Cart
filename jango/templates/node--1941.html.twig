      <script>
        jQuery(document).ready(function () {
          // Add smooth scrolling to all links
          jQuery("a").on('click', function (event) {
            // Make sure this.hash has a value before overriding default behavior
            if (this.hash !== "") {
              // Prevent default anchor click behavior
              event.preventDefault();
              // Store hash
              var hash = this.hash;
              // Using jQuery's animate() method to add smooth page scroll
              // The optional number (800) specifies the number of milliseconds it takes to scroll to the specified area
              jQuery('html, body').animate({
                scrollTop: jQuery('[name="' + jQuery.attr(this, 'href').substr(1) + '"]').offset().top
              }, 800, function () {
                // Add hash (#) to URL when done scrolling (default click behavior)
                window.location.hash = hash;
              });
            } // End if
          });
        });


        let steps = jQuery('.step');
        let margin = 0;
        //Click right arrow to advance
        jQuery('.slidearrow.right').on('click', function () {
          margin -= 100;
          if (Math.abs((margin - 100) / steps.length) === 100) {
            jQuery(this).hide();
          } else {
            jQuery(this).show();
          }
          jQuery('.slidearrow.left').show();
          jQuery('.steprow').css({ 'margin-left': margin + '%' });
          jQuery('.step-quickSelect span[data-step=' + ((margin / -100) + 1) + ']').addClass('active');
          jQuery('.step-quickSelect span[data-step=' + ((margin / -100) + 1) + ']').siblings().removeClass('active');
        });
        //Click left arrow to go back
        jQuery('.slidearrow.left').on('click', function () {
          margin += 100;
          if (margin === 0) {
            jQuery(this).hide();
          } else {
            jQuery(this).show();
          }
          jQuery('.slidearrow.right').show();
          jQuery('.steprow').css({ 'margin-left': margin + '%' });
          jQuery('.step-quickSelect span[data-step=' + ((margin / -100) + 1) + ']').addClass('active');
          jQuery('.step-quickSelect span[data-step=' + ((margin / -100) + 1) + ']').siblings().removeClass('active');
        });
        //move on step quick select
        jQuery('.step-quickSelect span').on('click', function () {
          //change slide point
          margin = (-100 * jQuery(this).attr('data-step')) + 100;
          jQuery('.steprow').css({ 'margin-left': margin + '%' });
          jQuery(this).siblings().removeClass('active');
          jQuery(this).addClass('active');
          //change arrows
          if (jQuery(this).attr('data-step') == 1) {
            jQuery('.slidearrow.right').show();
            jQuery('.slidearrow.left').hide();
          } else if (jQuery(this).attr('data-step') == steps.length) {
            jQuery('.slidearrow.left').show();
            jQuery('.slidearrow.right').hide();
          } else {
            jQuery('.slidearrow.left').show();
            jQuery('.slidearrow.right').show();
          }
        });
      </script>