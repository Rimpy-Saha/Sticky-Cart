function toggleForm() {
    var overlay = document.getElementById("overlay");
    var form = document.getElementById("ngs-form");

    // If the overlay is currently not displayed, show it
    if (overlay.style.display === "none" || overlay.style.display === "") {
        overlay.style.display = "flex";  // Show the overlay
        form.style.display = "block";    // Show the form
    } else {
        overlay.style.display = "none";  // Hide the overlay
        form.style.display = "none";     // Hide the form
    }
}

// Close the form if the user clicks outside the form
document.getElementById("overlay").addEventListener("click", function(event) {
    var form = document.getElementById("ngs-form");

    // Close the form if the click is outside the form (but inside the overlay)
    if (event.target === this) {
        toggleForm();  // This will close the form
    }
});




document.addEventListener('DOMContentLoaded', () => {


  
  // FAQ functionality
  const faqHeaders = document.querySelectorAll(".faq-header");

  faqHeaders.forEach(header => {
    const answer = header.nextElementSibling; 
    const arrow = header.querySelector(".faq-arrow"); 

    header.addEventListener("click", () => {
      const isOpen = answer.style.display === "block";
      
      answer.style.display = isOpen ? "none" : "block";
      header.classList.toggle("active");

      if (!isOpen) {
        arrow.style.transform = "rotate(-90deg)";
      } else {
        arrow.style.transform = "rotate(0deg)";
      }
    });
  });

  // Image description functionality
  const imageDescriptions = {
    "Pie Chart - smallRNA.png": "<b>Figure 1.</b> Pie Chart:  Shows the distribution of reads after initial adapter trimming and quality filter steps. The majority of the reads will be used for alignment to the human genome.",
    "figure-8-ngs-(1).jpg": "<b>Figure 2.</b> Stacked Bar Graph: Shows the smallRNA biotype composition per sample detected across the project.",
    "figure-2-ngs-(1).jpg": "<b>Figure 3.</b> Clustered Heatmap: Top 50 miRNAs are clustered together based on sample similarity to visualize differences in expression. Samples with higher miRNA expression to the mean expression are represented with green while lower expression is in red.",
    "figure-6-ngs-(1).jpg": "<b>Figure 4.</b> PCA (Principle Component Analysis) visualizes the similarity of samples within a group and the differences between groups in 2 dimensions.",
    "volcano_plot.png": "<b>Figure 5.</b> Volcano Plot shows the differential expressed miRNA between the samples, with the X-axis representing log2 fold change and the Y-axis showing -log10 p-value. Red dots represent significantly upregulated miRNAs and the green dots represent the downregulated miRNAs.",
    "figure-5-ngs-(1).jpg": "<b>Figure 6.</b> GO bubble plot: shows the gene function type (as  biological processes, molecular functions, or cellular components), and regulation level of the gene between the samples. The X-axis shows fold enrichment or GeneRatio, while the Y-axis lists enriched GO terms.",

    
  };

  const gridImages = document.querySelectorAll('.graphs img'); 
  const mainImage = document.getElementById('main-image');
  const imageLink = document.getElementById('image-link');

  gridImages.forEach(image => {
    image.addEventListener('click', () => {
      mainImage.style.opacity = 0; 

      setTimeout(() => {
        mainImage.src = image.src;
        mainImage.alt = image.alt;
        mainImage.style.opacity = 1;
        imageLink.href = image.src; 

        const figdescription = imageDescriptions[image.id]; 
        document.querySelector('.fig-description').innerHTML = figdescription; 
      }, 300); 
    });
  });



  // Cell expand/collapse functionality
const cells = document.querySelectorAll('.cell');

document.querySelectorAll('.cell a').forEach(link => {
  link.addEventListener('click', (event) => {
    event.stopPropagation(); // Stops the event from bubbling up to the .cell
  });
});

cells.forEach(cell => {
  cell.addEventListener('click', () => {
    const content = cell.querySelector('.cell-content');

    // Check if content exists
    if (content && content.textContent.trim() !== '') {
      // Close all other active cells smoothly
      cells.forEach(otherCell => {
        if (otherCell !== cell && otherCell.classList.contains('active')) {
          otherCell.classList.remove('active');
          otherCell.querySelector('.cell-content').style.maxHeight = '0';
        }
      });

      // Toggle the current cell
      cell.classList.toggle('active');

      if (cell.classList.contains('active')) {
        content.style.maxHeight = '0px'; // Set to 0 first
        setTimeout(() => {
          content.style.maxHeight = content.scrollHeight + 'px'; // Apply height after a tiny delay
        }, 10); 
      } else {
        content.style.maxHeight = '0';
      }
    }
  });
});

});


  </script>
