        <!-- Dynamic Contact Pictures -->
        <script>
          function getRandomInt(max) {
            return Math.floor(Math.random() * max);
          }
          let people = document.querySelectorAll('.contact-grid div.contact-img');
          if (people) {
            document.addEventListener("DOMContentLoaded", function (e) {
              let person = getRandomInt(3);
              people.forEach((element, index) => {
                if (index == person) {
                  element.classList.remove('hide');
                }
                else element.classList.add('hide');
              });
            })
          }

          var user = {{ logged_in|json_encode() }};

          if( user != true ){
              document.addEventListener("DOMContentLoaded", function(event) {
              // Your code to run since DOM is loaded and ready
                var y = window.location.href;
                var x = getCookie('registerdiv');
              
            
                if(x != 'yes'){
                  document.getElementsByClassName("register-div")[0].style.display = "block";
                }
              });
          }

          function close_registerdiv(){
            document.getElementsByClassName("register-div")[0].style.display = "none";
            setCookie('registerdiv','yes',1);
          }
  
          function setCookie(name,value,days) {
            var expires = "";
            if (days) {
              var date = new Date();
              date.setTime(date.getTime() + (days*24*60*60*1000));
              expires = "; expires=" + date.toUTCString();
            }
            document.cookie = name + "=" + (value || "")  + expires + "; path=y";
          }
  
          function getCookie(name) {
            var nameEQ = name + "=";
            var ca = document.cookie.split(';');
            for(var i=0;i < ca.length;i++) {
                var c = ca[i];
                while (c.charAt(0)==' ') c = c.substring(1,c.length);
                if (c.indexOf(nameEQ) == 0) return c.substring(nameEQ.length,c.length);
            }
            return null;
          }
          
  
        </script>