        }

        .step:nth-child(2) {
          -ms-grid-column: 2;
        }

        .step:nth-child(3) {
          -ms-grid-column: 3;
        }

        .step:nth-child(4) {
          -ms-grid-column: 4;
        }

        .step:nth-child(5) {
          -ms-grid-column: 5;
        }

        .step:nth-child(6) {
          -ms-grid-column: 6;
        }

        .step {
          background: #fff;
          padding: 0;
          border-radius: 10px;
          width: 98%;
          text-align: center;
          box-shadow: 2px 2px 3px rgba(0, 0, 0, 0.3);
          display: -ms-grid;
          -ms-grid-columns: 1fr 40px 1.5fr;
          display: grid;
          grid-template-columns: 1fr 1.5fr;
          grid-gap: 40px;
          text-align: left;
        }

        .step-image {
          -ms-grid-column: 1;
        }

        .step-info {
          -ms-grid-column: 3;
        }

        .step-quickSelect span {
          display: inline-block;
          margin: 20px 20px 0 20px;
          border: 1px solid #bbb;
          background: #fff;
          color: #555;
          cursor: pointer;
          width: 60px;
          height: 60px;
          line-height: 60px;
          border-radius: 50%;
          position: relative;
          font-family: 'Montserrat';
          font-size: 26px;
          font-weight: 500;
          text-align: center;
        }

        .step-quickSelect span:before {
          content: '';
          position: absolute;
          top: -5px;
          left: -5px;
          border: 1px solid #be1e2d;
          width: calc(100% + 10px);
          height: calc(100% + 10px);
          border-radius: 50%;
          opacity: 0.7;
        }

        .step-quickSelect span:after {
          content: '';
          position: absolute;
          top: -10px;
          left: -10px;
          border: 1px dashed #be1e2d;
          width: calc(100% + 20px);
          height: calc(100% + 20px);
          border-radius: 50%;
          opacity: 0.5;
        }

        .step-quickSelect span.active {
          border-color: #be1e2d;
          background: linear-gradient(to bottom right, #d73342, #8e111d);
          color: #fff;
        }

        .step-quickSelect span:not(.active):hover {
          background: linear-gradient(to bottom right, #fff, #d2d2d2);
        }

        .interim {
          margin: 0 5px;
          display: inline-block;
          position: relative;
          top: -5px;
        }

        .interim .dot {
          background: #be1e2d;
          width: 10px;
          height: 10px;
          display: inline-block;
          margin-right: 8px;
          border-radius: 50%;
        }

        .interim .dot.light {
          opacity: 0.25;
        }

        .interim .dot.mid {
          opacity: 0.6;
        }

        .interim .dot.dark {
          margin-right: 0;
        }

        .step-image {
          margin: 30px 0 30px 30px;
          text-align: center;
        }

        .step-image img {
          height: 100%;
          object-fit: contain;
        }

        .step-info {
          position: relative;
          padding: 30px 30px 30px 0;
          border-bottom-right-radius: 10px;
          overflow: hidden;
          border-top-right-radius: 10px;
        }

        .step strong {
          position: absolute;
          right: -150px;
          top: 0;
          height: 100%;
          width: 300px;
          background: #0f6b7a;
          border-radius: 50%;
          display: grid;
          align-items: center;
          text-align: center;
          font-size: 200px;
          color: #fff;
          font-family: 'Ubuntu', sans-serif;
          padding-right: 50px;
          padding-bottom: 15px;
          opacity: 0.4;
        }

        .step-info> :not(strong) {
          max-width: calc(100% - 180px) !important;
        }

        .step-info>b {
          display: inline-block;
          margin: 10px 0 20px;
          font-family: 'Monsterrat', sans-serif;
          font-size: 1.8rem;
          font-weight: 600;
          color: #222;
