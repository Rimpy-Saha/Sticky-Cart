    --institutions-loop-end: calc(-1*((150px + 2em + 1px)*var(--number-of-logos)) - 1em - 15px - (1px*(var(--number-of-logos))));
}

@keyframes institutions-loop{
  0%{
        left: var(--institutions-loop-start);
  }
  100%{
        left: var(--institutions-loop-end);
  }
}
  .institution-logos-container {
    left: 0;
    right: 0;
    margin: auto;
    overflow: hidden;
}
.institution-logos-container {
    left: 15px;
    white-space: nowrap;
    gap: var(--institutions-logos-gap);
    position: relative;
}
.institution-logos-container::before {
    content: '';
    background-image: linear-gradient(to right, rgb(255, 255, 255), rgba(255, 255, 255, 0));
    width: 10%;
    height: 100%;
    z-index: 1;
    position: absolute;
    left: 0;
    margin: auto;
}
.institution-logos-container::after {
    content: '';
    background-image: linear-gradient(to right, rgba(255, 255, 255, 0), rgba(255, 255, 255, 1));
    width: 10%;
    height: 100%;
    z-index: 1;
    position: absolute;
    right: 0;
    margin: auto;
}
 .institution-logos:first-child {
    margin-right: var(--institutions-logos-gap);
}
 .institution-logos {
    position: relative;
    left: 0;
    display: inline-block;
    white-space: nowrap;
