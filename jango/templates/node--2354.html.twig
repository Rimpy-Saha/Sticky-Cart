function hide_all_drawers(){
  var drawers = document.getElementsByClassName("drawer");
  for (var i = 0; i < drawers.length; i++){
    if(drawers[i].style.display == "block"){
      drawers[i].style.display = "none";
      var grand = drawers[i].parentElement;
      var more = grand.getElementsByClassName("more");
      more[0].style.display = "block";
      document.getElementById("overlay").style.display = "none";
      // drawers[i].style.zIndex = "1";
    }
  
  }
  var viewbtn = document.getElementsByClassName("view_btn");
  for (var j = 0; j < viewbtn.length; j++){
    if(viewbtn[j].style.opacity == 0){
      viewbtn[j].style.opacity = 1;
    }
  }
}
