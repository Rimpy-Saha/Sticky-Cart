 function slidingoptions(elem, elem1){

  var mainelem = elem1.parentElement.parentElement;
  if(elem == "right"){
    
    var slider_wrapper = mainelem.getElementsByClassName("slider-wrapper");
    
    var options = slider_wrapper[0].getElementsByClassName("options");
    var items = options[0].getElementsByClassName("btn2");
  
  if(window.innerWidth > 700){
    
