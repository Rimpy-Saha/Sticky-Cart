            /* promo section load animations*/
            .promo-section {
                opacity: 0;
                transform: translateY(40px);
                transition: opacity 0.6s ease-out, transform 0.6s ease-out;
            }
            .promo-section.visible {
                opacity: 1;
                transform: translateY(0);
            }
            .promo-section::after.visible {
                opacity:0;
            }
            /* Inner element animations */
            .promo-text h2,
            .promo-text .main-message,
            .sample-grid,
            .promo-btn,
            .promo-note,
            .promo-image {
                opacity: 0;
                transform: translateY(20px);
                transition: opacity 0.6s ease-out, transform 0.6s ease-out;
            }

            .promo-section.visible .promo-text h2 {
                transition-delay: 0.1s;
                opacity: 1;
                transform: translateY(0);
            }
            .promo-section.visible .promo-btn {
                transition-delay: 0.5s;
                opacity: 1;
                transform: translateY(0);
            }
            .promo-section.visible .promo-text .main-message {
                transition-delay: 0.2s;
                opacity: 1;
                transform: translateY(0);
            }

            .promo-section.visible .sample-grid {
                transition-delay: 0.3s;
                opacity: 1;
                transform: translateY(0);
            }
            .promo-section.visible .promo-note {
                transition-delay: 0.4s;
                opacity: 1;
                transform: translateY(0);
            }
            .promo-section.visible .promo-image {
                transition-delay: 0.5s;
                opacity: 1;
                transform: translateY(0);
            }

            /* Key points section load animations*/

            /* Fade-in base styles */
            .keypoint-section {
                opacity: 0;
                transform: translateY(40px);
                transition: opacity 0.6s ease-out, transform 0.6s ease-out;
            }
            .keypoint-section.visible {
                opacity: 1;
                transform: translateY(0);
            }

            /* Inner box animations */
            .keypoint-box {
                opacity: 0;
                transform: translateY(30px);
                transition: opacity 0.6s ease-out, transform 0.6s ease-out;
            }
            .keypoint-box.visible {
                opacity: 1;
                transform: translateY(0);
            }

            .keypoint-box .feature {
                opacity: 0;
                transform: translateY(15px);
                transition: opacity 0.4s ease-out, transform 0.4s ease-out;
            }
            .keypoint-box.visible .feature {
                opacity: 1;
                transform: translateY(0);
            }
            .keypoint-box.visible .feature:nth-child(1) { transition-delay: 0.1s; }
            .keypoint-box.visible .feature:nth-child(2) { transition-delay: 0.2s; }
            .keypoint-box.visible .feature:nth-child(3) { transition-delay: 0.3s; }
            .keypoint-box.visible .feature:nth-child(4) { transition-delay: 0.4s; }
            .keypoint-box.visible .feature:nth-child(5) { transition-delay: 0.4s; }
            .keypoint-box.visible .feature:nth-child(6) { transition-delay: 0.4s; }

            /* Base state for table rows */
            .table-container tbody tr {
                opacity: 0;
                transform: translateY(20px);
                transition: opacity 0.4s ease, transform 0.4s ease;
            }

            /* Animate in when table is visible */
            .table-container.visible tbody tr {
                opacity: 1;
                transform: translateY(0);
            }

            /* Improved Products Section load animations*/

            /* Fade/slide base state */
            .products-title,
            .improved-products-title,
            .svg-container,
            .improved-products,
            .table-label,
            .product-category h3,
            .table-container {
                opacity: 0;
                transform: translateY(30px);
                transition: opacity 0.6s ease-out, transform 0.6s ease-out;
            }

            /* Triggered state */
            .products-title.visible,
            .improved-products.visible, .improved-products-title, 
            .svg-container.visible,
            .table-label.visible,
            .product-category h3,
            .table-container.visible {
                opacity: 1;
                transform: translateY(0);
            }



            /* Initial hidden state */
            .improved-products .card {
                opacity: 0;
                transform: translateY(30px);
                transition: opacity 0.6s ease, transform 0.6s ease;
            }

            /* Final visible state */
            .improved-products .card.visible {
                opacity: 1;
                transform: translateY(0);
            }
