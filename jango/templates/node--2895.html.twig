document.addEventListener("DOMContentLoaded", function () {
  const sectionTitle = document.querySelector(".keypoint-section");
  const contentContainers = document.querySelectorAll(".keypoint-container");

  // Animate title when it enters the viewport
  const titleObserver = new IntersectionObserver((entries, observer) => {
    entries.forEach(entry => {
      if (entry.isIntersecting) {
        entry.target.classList.add("visible");
        observer.unobserve(entry.target);
      }
    });
  }, { threshold: 0.1 });

  // Animate each keypoint box inside its container when it appears
  const boxObserver = new IntersectionObserver((entries, observer) => {
    entries.forEach(entry => {
      if (entry.isIntersecting) {
        const box = entry.target.querySelector(".keypoint-box");
        if (box) {
          box.classList.add("visible");
        }
        observer.unobserve(entry.target);
      }
    });
  }, { threshold: 0.1 });

  // Start observing the title and all container sections
  if (sectionTitle) titleObserver.observe(sectionTitle);
  contentContainers.forEach(container => boxObserver.observe(container));
});



    document.addEventListener("DOMContentLoaded", function () {
    const title = document.querySelector(".products-title");
    const svg = document.querySelector(".svg-container");
    const label = document.querySelector(".table-label");
    const table = document.querySelector(".table-container");

    const observer = new IntersectionObserver((entries) => {
        entries.forEach(entry => {
        if (entry.isIntersecting) {
            entry.target.classList.add("visible");

            //  Extra: Add stagger animation for table rows once the container is visible
            if (entry.target === table) {
            const rows = table.querySelectorAll("tbody tr");
            rows.forEach((row, i) => {
                row.style.transitionDelay = `${i * 100}ms`;
            });
            }
        }
        });
    }, { threshold: 0.2 });

    [title, svg, label, table].forEach(el => {
        if (el) observer.observe(el);
    });
    });


    document.addEventListener("DOMContentLoaded", function () {
    const improvedSection = document.querySelector(".improved-products");
    const improvedCards = document.querySelectorAll(".improved-products .card");

    const observer = new IntersectionObserver((entries, observer) => {
        entries.forEach((entry, index) => {
        if (entry.isIntersecting) {
            // Add .visible to the section (triggers title animation)
            if (entry.target.classList.contains("improved-products")) {
            entry.target.classList.add("visible");
            }

            // Add .visible to each card with stagger
            if (entry.target.classList.contains("card")) {
            setTimeout(() => {
                entry.target.classList.add("visible");
            }, index * 150);
            }

            observer.unobserve(entry.target); // only run once
        }
        });
    }, {
        threshold: 0.1
    });

    if (improvedSection) observer.observe(improvedSection);
    improvedCards.forEach(card => observer.observe(card));
    });



