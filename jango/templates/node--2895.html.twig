            updateTable("All", centerCircle);
        });

        svgParts.forEach(part => {
            part.addEventListener("click", function () {
                if (selectedElement) {
                    selectedElement.classList.remove("selected-svg");
                }

                this.classList.add("selected-svg");
                selectedElement = this;

                // Only reset text scale if it was enlarged
                if (isTextEnlarged) {
                    centerText.animate([
                    { fontSize: '33px' },
                    { fontSize: '30px' }
                    ], {
                    duration: 400,
                    easing: 'ease-out',
                    fill: 'forwards' // makes it stay enlarged
                });      
                    isTextEnlarged = false;
                }

                updateTable(
                    this.id === "urine-svg" ? "Urine" :
                    this.id === "saliva-svg" ? "Saliva" :
                    "Plasma/Serum",
                    this
                );
            });
        });
    });


