    document.addEventListener("DOMContentLoaded", function () {
        const centerCircle = document.getElementById("center-circle");
        const centerText = document.getElementById("center-text");
        let isTextEnlarged = false; // New flag

        const svgParts = [
            document.getElementById("urine-svg"),
            document.getElementById("saliva-svg"),
            document.getElementById("plasma-serum-svg")
        ];

        centerCircle.addEventListener("click", function () {
            if (selectedElement) {
                selectedElement.classList.remove("selected-svg");
            }

            centerCircle.classList.add("selected-svg");
            selectedElement = centerCircle;

            centerCircle.animate([
                { transform: 'scale(1)' },
                { transform: 'scale(1.1)' }
            ], {
                duration: 400,
                easing: 'ease-out'
            });

            // Animate text only if not already enlarged
            if (!isTextEnlarged) {
                    centerText.animate([
                    { fontSize: '30px' },
                    { fontSize: '33px' }
                    ], {
                    duration: 400,
                    easing: 'ease-out',
                    fill: 'forwards' // makes it stay enlarged
                });
                isTextEnlarged = true;
