<script>

    function  toggleShownSection(selected_section){
        const all_sections = document.querySelectorAll('.product-listings .product-category');
        all_sections.forEach(element => {
            element.classList.remove('active');
        });
        selected_section.classList.add('active');
    }

    const select = document.querySelector('#select-product-cat');
    select.addEventListener('change', function(e){
        let selected_section = document.querySelector('#'+select.value);
        if(selected_section){
            toggleShownSection(selected_section);
        }
    })

    const clickable_svgs = document.querySelectorAll('.purification-product-rings > svg .hover-ring');
    clickable_svgs.forEach(clickable_svg => {
        clickable_svg.addEventListener('click', function(e){
            let select_list = document.querySelector('#select-product-cat');
            if(select_list){
                select_list.value=clickable_svg.getAttribute('section');
                let selected_section = document.querySelector('#'+select_list.value);
                if(selected_section){
                    toggleShownSection(selected_section);
                }
            }
        })
    });
</script>