        }

        .stat {
          width: 100%;
          max-width: 340px;
          text-align: center;
          background: #eee;
          padding: 30px 30px 20px;
          border-radius: 10px;
          box-shadow: 2px 2px 3px rgba(0, 0, 0, 0.3);
        }

        .stat>strong img {
          height: 75px;
          position: relative;
        }

        .stat>strong {
          display: block;
          margin: 0 auto;
          border-radius: 50%;
          position: relative;
          margin-bottom: 20px;
          text-align: center;
        }

        .stat>strong:before {
          /*content: '';*/
          position: absolute;
          top: -5px;
          left: -5px;
          border: 1px solid #be1e2d;
          width: calc(100% + 10px);
          height: calc(100% + 10px);
          border-radius: 50%;
          opacity: 0.7;
        }

        .stat>strong:after {
          /*content: '';*/
          position: absolute;
          top: -10px;
          left: -10px;
          border: 1px dashed #be1e2d;
          width: calc(100% + 20px);
          height: calc(100% + 20px);
          border-radius: 50%;
          opacity: 0.5;
        }

