          if (email != null && email != "") {
            if (email.match(validRegex)) {


              var slide = document.querySelectorAll(".form-table");
              // console.log(slide);
              if (elem == "next") {

                // slide[0].style.left = "-"+leftval+"%";
                var leftval = 100 * count;
                for (var j = 0; j < slide.length; j++) {

                  slide[j].style.left = "-" + leftval + "%";



                }
                count++;
              }
