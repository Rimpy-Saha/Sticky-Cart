        info_div.innerHTML += `
        <ul class="contact">
          ${distributor.contact}
        </ul>`;
      }
      distributorBlock.append(info_div);
      if (distributor.logo != ""){
        let dist_img = document.createElement("img");
        dist_img.src=distributor.logo;
        distributorBlock.append(dist_img);
      } 
      document.querySelector("#distributor-results-wrapper").append(block_to_append);
