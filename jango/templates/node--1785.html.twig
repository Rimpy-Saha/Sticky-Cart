        width: 85vw;
      }
    }
  
    @media only screen and (max-width: 300px) {
      .button_div {
        justify-content: flex-start;
        flex-direction: column;
        align-items: flex-start;
      }
  
      .card-outside {
        width: 135vw;
      }
  
      .icon {
        margin-top: 5%;
      }
  
      .company {
        width: min-content;
      }
    }
  
    .anchorSection {
      padding: 0px 0px;
    }
  
    .overlay-form,
    .overlay-thankyou {
      position: fixed;
      display: none;
      width: 100%;
      height: 100%;
      top: 0;
      left: 0;
      right: 0;
      bottom: 0;
      background-color: rgba(0, 0, 0, 0.5);
      z-index: 2;
      cursor: pointer;
    }
  
    label {
      margin: 2% 0;
    }
  
    @media only screen and (max-width: 721px) {
      .dist-form {
        /* background: white; */
        padding: 0% 3%;
      }
  
      .dist-form>div>img {
        width: 100px;
        display: none;
      }
  
      .form-section {
        top: 11%;
      }
  
      input[type="text"],
      input[type="email"] {
        padding: 0;
      }
    }
  
    .name_div:hover {
      cursor: pointer;
    }
  
    #error_file {
      color: red;
    }
  
    .distributors-results {
      grid-area: b;
    }
  
    .hide-company {
      display: none;
    }
  
    /* new styles*/
    #drop-down-continents,
    #drop-down-country-list {
      outline: none;
      border-radius: 4px;
      max-width: 400px;
    }
  
    #drop-down-country-list {
      /* margin-top: 20px; */
    }
  
    .dispay-country {
      display: block;
    }
  
    #select-country,
    #select-continent {
      margin-bottom: 5px;
    }

    #select-country {
      margin-top: 10px;
      max-width:300px;
    }
