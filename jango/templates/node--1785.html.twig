      </select>
    </div>
    <div class="grid-panel">
      {% if restricted_country %} {% else %}
      <div class="interested" id="interested-id">
        <img src="/sites/default/files/images/distributors/distribution.png" />
        <h3>Interested in becoming a Norgen Biotek Distributor?</h3>
        <a class="btn btn-contact" href="/distributor-application-form">Apply today</a>
      </div>
      {% endif %}
      <div class="distributors-results" id="distributor-results-wrapper" name="distributor-results-wrapper">
      
      </div>
    </div>
  </section>
  
