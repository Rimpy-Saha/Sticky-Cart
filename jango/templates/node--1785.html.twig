    available_countries = [];
  
    distributors.forEach((distributor) => {
      /* add countries to the available_countries array */
      distributor.coverage.forEach(country => {
        if(!available_countries.includes(country.trim())){
          available_countries.push(country.trim());
        }
      });
      let website_link = null;
      if(distributor.website != undefined){
        website_link = document.createElement("a");
        website_link.target = "_blank";
        website_link.href = distributor.website;
      }

      let distributorBlock = document.createElement("div");
      distributorBlock.classList.add("dist-box");
      block_to_append = distributorBlock;
      if(website_link!=null) {
        website_link.append(distributorBlock);
        block_to_append = website_link;
      }
      
      let info_div = document.createElement("div");
      info_div.classList.add("info");
      info_div.innerHTML += `<h3 class="name notranslate">${distributor.name}</h3>
      <div class="countries">
        ${distributor.coverage.join(', ')}
      </div>
      <div class="flags">
        ${distributor.coverageFlags}
      </div>`;
