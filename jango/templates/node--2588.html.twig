<style>
    /* General */
    #block-jango-content h1, #block-jango-content h2{
        font-weight:bold;
    }
    #block-jango-content h2{
        font-size:28px;
    }
    #block-jango-content section{
        padding:80px 0px;
    }
    /* Main Intro Section */
    #main-intro #advantages-list{
        list-style-type: none;
        display:grid;
        grid-template-columns: 1fr 1fr;
        grid-gap:40px;
        color:white;
        margin-top:80px;
        padding-left:0px;
    }
    #main-intro #advantages-list li{
        display: grid;
        grid-template-columns: 50px 1fr;
        grid-gap: 20px;
    }
    #main-intro #advantages-list li strong{
        font-size:24px;
    }
    #advantages-row{
        position:relative;
    }
    .advantages-icon{
        width:50px;
        height:50px;
    }
    /* Product Listings Section */
    #product-listings #products-grid {
        display: grid;
        grid-template-columns: 1fr 1fr 1fr;
        align-items: end;
        gap: 20px;
        margin: 75px 0 0;
    }
    #product-filters{
        border:1px solid #f6f6f6;
        border-radius:12px;
        padding:20px;
    }
    .sample-filters h5{
        font-size:18px;
    }
    .sample-filters h6{
        font-size:14px;
    }
    .sample-filters-list{
        list-style-type:none;
        display:flex;
        justify-content:space-between;
        padding-left:0px;
    }
    ul.analyte-filters-list {
        list-style-type: none;
    }
    .sample-filters-list > li{
        width:100%;
    }
    .sample-filters-list .sample-filter-option.active > h4, .sample-filters-list .sample-filter-option:hover h4,
    .all-products.active h3, .all-products:hover h3{
        font-weight: bold;
    }
    .sample-filters-list .sample-filter-option + .analyte-filters-list{
        visibility: hidden;
        opacity:0;
        height:0px;
        padding-left:10px;
    }
    .sample-filters-list .sample-filter-option.active + .analyte-filters-list{
        visibility:visible;
        opacity:1;
        height:auto;
    }
    div#product-listings button {
        background-color: transparent;
        border: none;
        width:100%;
        text-align:center;
    }
    div#product-listings button.sample-filter-option[value="blood"]{
        background-color: #ffc8cc;
    }
    div#product-listings button.sample-filter-option[value="blood"] + .analyte-filters-list{
        background-color:#ffc8cc70;
    }
    div#product-listings button.sample-filter-option[value="plasma"]{
        background-color: #fff5ca;
    }
    div#product-listings button.sample-filter-option[value="plasma"] + .analyte-filters-list{
        background-color: #fff5ca70;
    }
    div#product-listings button.sample-filter-option[value="buffy"]{
        background-color: #fffdef;
    }
    div#product-listings button.sample-filter-option[value="buffy"] + .analyte-filters-list{
        background-color: #fffdef70;
    }
    div#product-listings button:hover{
        box-shadow: inset 0 3px 5px rgba(0,0,0,0.125);
    }
    /* Interactive SVG Tube */
    /* Need this weird hover code for the whole blood because it's layers are sandwiching the tube's highlight rectangle */
    .svg-hover-element:hover > .content{
        opacity: 0.7;
        cursor: pointer;
        animation-play-state: unset;
    }
    .svg-hover-element:hover > .content > *:not(.ignore-stroke):not(text),
    .svg-hover-element:hover > .content#blood-line, 
    .svg-hover-element:hover > .content > #plasma-shadow, 
    .svg-hover-element:hover > .content > #buffy-shadow{
        stroke:black;
        stroke-width:2px;
    }
    .svg-hover-element:hover > .content > #plasma-shadow{
        stroke-dasharray: 18 111.02;
    }
    .svg-hover-element:hover > .content > #buffy-shadow{
        stroke-dasharray: 18 18;
    }
    .svg-hover-element:hover ~ .svg-hover-element > .content{
        animation-play-state: unset;
    }
    .svg-hover-element#plasma-label > .content{
        animation: indicateClickable1 2s infinite;
    }
    .svg-hover-element > .content{
        animation: indicateClickable2 2s infinite;
    }
    #red-blood-label, #centrifuge-label{
        opacity: 40%;
        filter: saturate(0.5);
    }
    /* Promotion Structure Section */
    #promo-structure, #other-promos {
        background-color: #f6f6f6;
    }
    #consultation{
        background-color:var(--norgen-primary-blue-light);
    }
    #promo-structure .promo-cards{
        display:grid;
        grid-template-columns: 1fr 1fr;
        grid-gap: 20px;
    }
    #promo-structure .promo-cards .promo-card{
        border-radius:12px;
        background-color:white;
        padding:30px;
        border: 1px solid #80808033;
        box-shadow: rgba(149, 157, 165, 0.2) 0px 8px 24px;
    }
    #promo-structure .promo-cards .promo-card.bonus{
        grid-column: 1 / 3;
    }
    #promo-structure .promo-cards strong{
        font-size:28px;
    }
    #promo-structure .promo-cards .promo-card.bonus > .bonus-grid{
        display:flex;
        justify-content: center;
        align-items:center;
    }
    /* Other Promotions */
    .other-promos-grid{
        display: grid;
        grid-template-columns: 1fr 1fr 1fr;
        gap: 10px;
        margin-top:30px;
    }
    .other-promos-grid > a{
        padding: 10px 20px;
        border-radius: 12px;
        border: 1px solid #80808033;
        box-shadow: rgba(149, 157, 165, 0.2) 0px 8px 24px;
        background-color: white;
    }
    .other-promos-grid img{
    width:25px;
    }
    .other-promos{
    padding:100px 0;
    }
    /* Trusted Partners */
    :root {
        --number-of-logos: 16;
        --logo-width: 150px;
        --institutions-logos-gap: 2em;
        --institutions-loop-start: 0px;
        --institutions-loop-end: calc(-1*((150px + 2em + 1px)*var(--number-of-logos)) - 1em - 15px - (1px*(var(--number-of-logos))));
    }

    .institution-logos-container {
        left: 0;
        right: 0;
        margin: auto;
        overflow: hidden;
    }
    .institution-logos-container {
        left: 15px;
        white-space: nowrap;
        gap: var(--institutions-logos-gap);
        position: relative;
    }
    .institution-logos-container::before {
        content: '';
        background-image: linear-gradient(to right, rgb(255, 255, 255), rgba(255, 255, 255, 0));
        width: 10%;
        height: 100%;
        z-index: 1;
        position: absolute;
        left: 0;
        margin: auto;
    }
    .institution-logos-container::after {
        content: '';
        background-image: linear-gradient(to right, rgba(255, 255, 255, 0), rgba(255, 255, 255, 1));
        width: 10%;
        height: 100%;
        z-index: 1;
        position: absolute;
        right: 0;
        margin: auto;
    }
    .institution-logos:first-child {
        margin-right: var(--institutions-logos-gap);
    }
    .institution-logos {
        position: relative;
        left: 0;
        display: inline-block;
        white-space: nowrap;
        margin-top: 42px;
        height: 75px;
        padding-left: 20px; 
        padding-right: 20px; 
        width: calc(((150px + 2em + 1px)*var(--number-of-logos)) + 1em);
        animation: institutions-loop 30s infinite linear;;
    }
    .institution-logos img {
        width: var(--logo-width);
        object-fit: contain;
        display: inline;
        height: 75px;
        margin-right: var(--institutions-logos-gap);
    }

    /* Animations */
    @keyframes indicateClickable1{
        50%{
            transform: translateY(-8px);
            filter:brightness(1.05);
        }
    }
    @keyframes indicateClickable2{
        50%{
            transform: translateY(-4px);
            filter:brightness(1.05);
        }
    }

    @keyframes institutions-loop{
        0%{
                left: var(--institutions-loop-start);
        }
        100%{
                left: var(--institutions-loop-end);
        }
    }

    /* Media Queries */
    @media only screen and (max-width:991px){
        #advantages-tube{
            position:absolute;
            right:0;
            bottom:-40px;
            opacity:0.2;
            z-index:-1;
        }
    }
    @media only screen and (max-width:600px){
        #product-listings #products-grid, 
        .other-promos-grid, 
        #promo-structure .promo-cards{
            grid-template-columns: 1fr;
        }
        #promo-structure .promo-cards .promo-card.bonus{
            grid-column: 1 / 2;
        }
        #promo-structure .promo-cards .promo-card.bonus > .bonus-grid{
            flex-direction:column;
        }
        .sample-filters-list{
            flex-direction:column;
        }
        #main-intro #advantages-list{
            grid-template-columns:1fr;
        }
    }
</style>


