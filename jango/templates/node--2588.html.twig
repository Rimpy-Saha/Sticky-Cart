<script>
    function filterSample(sample){
        products = document.querySelectorAll('#products-grid .product-div');
        products.forEach(element => {
            if(sample == null || element.getAttribute('sample') == sample){
                element.style.display="block";
            }
            else{
                element.style.display="none";
            }
        });
    }
    function filterAnalyte(sample, analyte){
        active_checkboxes = analyte.parentElement.parentElement.querySelectorAll('.analyte-filter-option:checked');
        products = document.querySelectorAll('#products-grid .product-div');
        products.forEach(element => {
            show = false;
            skip_analyte = false;
            if(active_checkboxes.length == 0 || active_checkboxes == null){ // dont check the analyte if none are selected
                skip_analyte = true;
                if(element.getAttribute('sample') == sample){
                    show = true;
                }
            }
            if(!skip_analyte){
                for(const checkbox of active_checkboxes){
                    if(element.getAttribute('sample') == sample && element.getAttribute('analyte') == checkbox.value){
                        show = true;
                        break;
                    }
                }
            }
            if(show){
                element.style.display="block";
            }
            else{
                element.style.display="none";
            }
        });
    }
    function activateFilter(filter_button, other_filters){
        if(other_filters!=null){
            other_filters.forEach(element => {
                element.classList.remove('active');
            });
        }
        filter_button.classList.add('active');
    }
    function deactivateFilters(sample_filters, analyte_filters){
        sample_filters.forEach(element => {
            element.classList.remove('active');
        });
        analyte_filters.forEach(element => {
            element.classList.remove('active');
        });
    }
    function deactivateFilter(filter){
        filter.classList.remove('active');
    }

    sample_filters = document.querySelectorAll('.sample-filter-option');
    sample_filter_blood = document.querySelector('.sample-filter-option[value="blood"]');
    sample_filter_plasma = document.querySelector('.sample-filter-option[value="plasma"]');
    sample_filter_buffy = document.querySelector('.sample-filter-option[value="buffy"]');
    analyte_filters = document.querySelectorAll('.analyte-filter-option');
    reset_filter = document.querySelector('#reset-filters');

    /* SVG buttons */
    whole_blood_svg = document.querySelector('#whole-blood-label');
    if(whole_blood_svg) whole_blood_svg.addEventListener('click', function(e){
        filterSample('blood');
        activateFilter(sample_filter_blood, sample_filters);
    })
    plasma_svg = document.querySelector('#plasma-label');
    if(plasma_svg) plasma_svg.addEventListener('click', function(e){
        filterSample('plasma');
        activateFilter(sample_filter_plasma, sample_filters);
    })
    buffy_coat_svg = document.querySelector('#buffy-coat-label');
    if(buffy_coat_svg) buffy_coat_svg.addEventListener('click', function(e){
        filterSample('buffy');
        activateFilter(sample_filter_buffy, sample_filters);
    })

    sample_filters.forEach(element => {
        element.addEventListener('click', function(e){
            activateFilter(this, sample_filters);
            deactivateFilter(reset_filter);
            filterSample(this.value);
        })
    });
    analyte_filters.forEach(element => {
        element.addEventListener('change', function(e){
            if(this.parentElement.parentElement.parentElement.querySelector('.sample-filter-option')){
                filterAnalyte(this.parentElement.parentElement.parentElement.querySelector('.sample-filter-option').value, this);
            }
        })
    });
    reset_filter.addEventListener('click', function(e){
        filterSample(null);
        activateFilter(reset_filter, null);
        deactivateFilters(sample_filters, analyte_filters);
    });
</script>